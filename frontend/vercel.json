{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://artistsaid.vercel.app/api",
    "NEXT_PUBLIC_APP_URL": "https://artistsaid.vercel.app"
  }
}
